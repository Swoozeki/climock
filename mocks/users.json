{
  "feature": "users",
  "endpoints": [
    {
      "id": "get-user-profile",
      "method": "GET",
      "path": "/api/users/:id",
      "active": true,
      "defaultResponse": "error",
      "responses": {
        "error": {
          "status": 404,
          "headers": {
            "Content-Type": "application/json"
          },
          "body": {
            "code": "USER_NOT_FOUND",
            "error": "User not found"
          },
          "delay": 100
        },
        "premium": {
          "status": 200,
          "headers": {
            "Content-Type": "application/json"
          },
          "body": {
            "email": "john@example.com",
            "id": "{{params.id}}",
            "memberSince": "2020-01-01",
            "name": "John Doe",
            "premium": true
          },
          "delay": 200
        },
        "standard": {
          "status": 200,
          "headers": {
            "Content-Type": "application/json"
          },
          "body": {
            "email": "john@example.com",
            "id": "{{params.id}}",
            "name": "John Doe"
          },
          "delay": 200
        }
      }
    }
  ]
}